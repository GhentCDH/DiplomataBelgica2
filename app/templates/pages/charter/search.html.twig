{% extends 'base.html.twig' %}

{% set page_title %}Search charters{% endset %}

{% block body_class %}layout--fullscreen{% endblock %}

{% block main %}
    <charter-search-app
        id="charter-search-app"
        init-urls="{{ urls }}"
        title="{{ page_title }}"
    />
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {{ encore_entry_script_tags('charter-search') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('charter-search') }}
{% endblock %}