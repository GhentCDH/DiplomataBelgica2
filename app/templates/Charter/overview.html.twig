{% extends 'base.html.twig' %}

{% block title %}Diplomata Belgica - Search{% endblock %}

{% block body_class %}layout--fullscreen{% endblock %}

{% block content %}
    <charter-search-app
        id="charter-search-app"
        init-urls="{{ urls }}"
        title="{{ title }}"
    />
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {{ encore_entry_script_tags('charter-search') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('charter-search') }}
{% endblock %}