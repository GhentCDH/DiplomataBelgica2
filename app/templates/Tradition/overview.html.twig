{% extends 'base.html.twig' %}

{% block title %}Diplomata Belgica - Search{% endblock %}

{% block body_class %}layout--fullscreen{% endblock %}

{% block content %}
        <tradition-search-app
            id="tradition-search-app"
            init-urls="{{ urls }}"
            init-data="{{ data }}"
            init-identifiers="{{ identifiers }}"
            init-managements="{{ managements }}"
            title="{{ title }}"
        />
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {{ encore_entry_script_tags('tradition-search') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('tradition-search') }}
{% endblock %}
